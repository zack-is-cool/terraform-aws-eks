{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>defenseunicorns/narwhal-delivery-renovate-config:terraformReposAuto.json5"
  ],
  "customDatasources": {
    "vpc-cni": {
      "defaultRegistryUrlTemplate": "https://raw.githubusercontent.com/zack-is-cool/terraform-aws-eks/refs/heads/main/dependencies/eks-addons-versions.json",
      "format": "json",
      // "transformTemplates": [
      //   "{\"releases\": $map($.addons[addonName='vpc-cni'].addonVersions, function($v){ {\\\"version\\\": $v.addonVersion} })}"
      // ]
      "transformTemplates": [
        "{\"releases\":[{\"version\":\"v1.19.2-eksbuild.1\"}]}"
      ]
    }
  },
  "logLevelRemap": [
    {
      "matchMessage": "/.*/",
      "newLogLevel": "trace"
    }
  ],
  "forkProcessing": "enabled",
  "schedule": ["at any time"],
  "customManagers": [
    {
      // Which files to parse
      "fileMatch": [
        // Adjust as needed. For example: all .tf or .tfvars
        "(^|/).*\\.tfvars$"
      ],
      // Regex to find your pinned version in the code
      // e.g. `addon_version = "v1.19.0-eksbuild.1"`
      "matchStrings": [
        "addon_version\\s*=\\s*\"(?<currentValue>\\S+)\""
      ],

      // Tells Renovate to use the "vpc-cni" custom datasource above
      "datasourceTemplate": "custom.vpc-cni",
      "depNameTemplate": "vpc-cni",

      // Usually you want semver for AWS versions
      "versioningTemplate": "loose"
    }
  ]
}
